{
  "symbol-line.separator": " > ",
  "symbol-line.maxItems": 4,
  "symbol-line.showArrays": false,
  "symbol-line.showVariables": false,
  "symbol-line.showKeys": false,
  "symbol-line.showNull": false,
  "symbol-line.showFiles": true,
  "symbol-line.showModules": false,
  "symbol-line.showPackages": false,
  "symbol-line.showClasses": false,
  "symbol-line.showProperties": false,
  "symbol-line.showFields": false,
  "symbol-line.showMethods": false,
  "symbol-line.showEnums": false,
  "symbol-line.showEvents": false,
  "symbol-line.showStrings": false,
  "symbol-line.showObjects": false,
  "symbol-line.showEnumMembers": false,
  "symbol-line.showNumbers": false,
  "symbol-line.showStructs": false,
  "symbol-line.showConstants": false,
  "symbol-line.showConstructors": false,
  "symbol-line.showTypeParameters": false,
  "symbol-line.showOperators": false,
  "symbol-line.showInterfaces": false,
  "symbol-line.showBooleans": false,
  "symbol-line.showFunctions": true,
  "outline.keepWindow": true,
  "Lua.diagnostics.globals": ["vim"],
  "translator.engines": [
    "youdao",
    "haici"
  ],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "cpp",
        "c"
      ],
      "rootPatterns": [
        "compile_commands.json",
        ".ccls",
        ".git/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/cache/ccls-cache"
        },
        "highlight": {
          "lsRanges": true
        },
        "client": {
          "snippetSupport": true
        },
        "clang": {
          "extraArgs": [
            "-std=c++17",
            "-Wall"
          ]
        }
      }
    }
  }
}
